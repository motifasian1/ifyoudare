
/**
 * This program actually works in finding all of the genes inside a string of DNA
 * 
 * @author JY
 * @version 1.0
 */
public class FinalQuizRedo {
    public String findGene(String dna, int where) {
        //find first occurrence of "ATG, startINdex
        int startIndex = dna.indexOf("ATG", where);
        //if startIndex is -1, return ""
        if (startIndex == -1) {
            return"";
        }
        int taaIndex = findStopCodon(dna,startIndex,"TAA");
        int tagIndex = findStopCodon(dna,startIndex,"TAG");
        int tgaIndex = findStopCodon(dna,startIndex,"TGA");
        int minIndex = 0;
        if (taaIndex == -1 || (tgaIndex != -1 && tgaIndex < taaIndex)) {
            minIndex = tgaIndex;
        }
        else {
            minIndex = taaIndex;
        }
        if (minIndex == -1 || tagIndex != -1 && tagIndex < minIndex) {
            minIndex = tagIndex;
        }
        if (minIndex == -1) {
            return "";
        }
        return dna.substring(startIndex, minIndex+3);
    }
    
    public void printAllGenes(String dna) {
        //set startIndex to 0
        int startIndex = 0;
        //count numGenes
        int numGenes = 0;
        //count large genes >60
        int largeGenes = 0;
        //count genes with high CG ratio
        int numGenesHighCGRatio = 0;
        //maxGeneLength stores the length of teh longest gene
        int maxGeneLength = 0;
        //Repeat the following steps
        while (true) {
            //Find the next gene after startIndex
            String currentGene = findGene(dna, startIndex);
            //If no gene was found, leave this loop
            if (currentGene.isEmpty()) {
                break;
            }
            //Increment numGenes
            numGenes++;
            //if currentGene > 60 than increment largeGenes
            if(currentGene.length()>60) {
                largeGenes++;
            }
            //if cgRatio > .35 increment cgRatio
            if (cgRatio(currentGene)>0.35) {
                numGenesHighCGRatio++; 
            }
            //compare maxGeneLength with currentGene and update 
            maxGeneLength = Math.max(maxGeneLength, currentGene.length());
            //Print that gene out
            System.out.println(currentGene);
            //Set startIndex to just past the end of the gene
            startIndex = dna.indexOf(currentGene, startIndex) + 
                         currentGene.length();
        }
        System.out.println("numGenes is " + numGenes);
        System.out.println("numlargeGenes is " + largeGenes);
        System.out.println("numGenesHighCGRatio is " + numGenesHighCGRatio);
        System.out.println("CTG count is " + countCTG(dna));
        System.out.println("maxGeneLength is " + maxGeneLength);
    }
    
    public int countCTG (String dna) {
        int count = 0;
        int currIndex = dna.indexOf("CTG");
        //If no stop codon, return out of bounds
        while (currIndex != -1 && currIndex != 0) {
            //update currIndex to find next occurrence of the stop codon
            count++;
            currIndex = dna.indexOf("CTG", currIndex+3);
            }
        return count;
    }
    
    public double cgRatio (String gene) {
        int numCG = 0;
        int currIndex = 0;
        float cgRatio = 0;
        //if charAT currIndex = C or G, then increment numCG
        for (int i=0; i<gene.length(); i++) {
            if (gene.charAt(i) == 'C' || gene.charAt(i) == 'G') {
                numCG++;
            }
        }
        return cgRatio = ((float) numCG) / gene.length();
    }
    
    public int findStopCodon(String dnaStr, int startIndex, String stopCodon) {
        //find stopCodon starting from (startIndex + 3), currIndex
        int currIndex = dnaStr.indexOf(stopCodon,startIndex+3);
        //as long as currIndex is not equal to -1
        while (currIndex != -1) {
            int diff = currIndex - startIndex;
            //check if currIndex - startIndex is a multiple of 3
            if (diff%3 == 0) {
                return currIndex; //if so, currIndex is answer, return it
            } //if not, update currIndex, looking for stopCodon, 
              //        starting from currIndex + 1
            else {
                currIndex = dnaStr.indexOf(stopCodon, currIndex+1);
            }
            //if we exit loop, we didn't find stopCodon
            //so return -1
        }
        return -1;
    }
    
    public void testFindStop() {
        //            01234567890123456789012345
        String dna = "xxxyyyzzzTAAxxxyyyzzzTAAxx";
        int dex = findStopCodon(dna,0,"TAA");
        if (dex != 9) System.out.println("error on 9");
        dex = findStopCodon(dna,9,"TAA");
        if (dex != 21) System.out.println("error on 21");
        dex = findStopCodon(dna,1,"TAA");
        if (dex != -1) System.out.println("error on 26");
        dex = findStopCodon(dna,0,"TAG");
        if (dex != -1) System.out.println("error on 26 TAG");
        System.out.println("tests finished");
    }
    
    public void testOn(String dna) {
        System.out.println("Testing printAllGenes on ");
        printAllGenes(dna);
    }
    
    public void test() {
        testOn("");
    }
}
