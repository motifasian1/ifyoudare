import edu.duke.*;
/**
 * Write a description of Part1 here.
 * 
 * @author (your name) 
 * @version (a version number or a date)
 */
public class Part1 {
    public String findSimpleGene(String dna, int where) {
        //Convert dna into all uppercase letters
        String newDNA = dna.toUpperCase();
        //Find index of start codon, store indexOfStart
        int indexOfStart = findStartCodon(newDNA, where); 
        //If no start codon "ATG" or stop codon, return empty string
        if (indexOfStart == -1 || dna == "") {
            return "";
        } 
        //Find index of stop codon, store indexOfStop
        int indexTAA = findStopCodon(newDNA, indexOfStart, "TAA");
        int indexTGA = findStopCodon(newDNA, indexOfStart, "TGA");
        int indexTAG = findStopCodon(newDNA, indexOfStart, "TAG");
        int minIndex = Math.min(indexTAA, Math.min(indexTGA, indexTAG));
        if (minIndex == dna.length()) {
            return "";
        }
        return newDNA.substring(indexOfStart, minIndex + 3);
    }
    
    public int findStartCodon (String dna, int where) {
        int indexOfStart = dna.indexOf("ATG", where);
        if (indexOfStart == -1) {
            return -1;
        }
        return indexOfStart;
    }
    
    public int findStopCodon (String dna, int startIndex, String stopCodon) {
        //Search dna, if "TAA" "TGA" or "TAG" are found, compare the index of each and return the smallest index
        int currIndex = dna.indexOf(stopCodon, startIndex + 3);
        //If no stop codon, return out of bounds
        if (currIndex != -1) {
            if ((currIndex - startIndex) % 3 == 0) {
                return currIndex;
            }
            //update currIndex to find next occurrence of the stop codon
            currIndex = dna.indexOf(stopCodon, currIndex+3);
        }
        return dna.length();
    }
    
    public void testFindStopCodon() {
        //Return index 9 for TAA stop 
        String dna = "ATGAAATTTTAA";
        String stop = "TAA";
        System.out.println(findStopCodon(dna, findStartCodon(dna, 0), stop));
        //Return 18 for index TAG
        dna = "ATGAAATTTCCCGGGAAATAGTAA";
        stop = "TAG";
        System.out.println(findStopCodon(dna, findStartCodon(dna, 0), stop));
        //Return index 15 for TGA
        dna = "ATGAAATTTCCCGGGTGAAAATAGTAA";
        stop = "TAG";
        System.out.println(findStopCodon(dna, findStartCodon(dna, 0), stop));
        //Return empty b/c no TAG found
        dna = "ATGAAATTTCCCGGGTATTAT";
        stop = "TAG";
        System.out.println(findStopCodon(dna, findStartCodon(dna, 0), stop));
    }
    
    public void testFindGene() {
        System.out.println("New test");
        //DNA no ATG
        String dna = "";
        System.out.println(dna);
        System.out.println(findSimpleGene(dna, 0));
    }
    
    public void printAllGenes(String dna) {
        //Set startIndex to 0
        int startIndex = 0;
        //Repeat the following steps
        while(true){
            //Find the next gene after startIndex
            String currGene = findSimpleGene(dna, startIndex);
            //If no gene found, leave this loop
            if(currGene.isEmpty()) {
                break;
            }
            //Print that gene out
            System.out.println("Current gene is: " + currGene);
            //Set startIndex to new index of end of last gene
            startIndex = dna.indexOf(currGene, startIndex) + currGene.length();
        }
    }
    
    public StorageResource getAllGenes(String dna) {
        StorageResource sr = new StorageResource();
        //Set startIndex to 0
        int startIndex = 0;
        //Repeat the following steps
        while(true){
            //Find the next gene after startIndex
            String currGene = findSimpleGene(dna, startIndex);
            //If no gene found, leave this loop
            if(currGene.isEmpty()) {
                break;
            }
            //Store gene in sr
            sr.add(currGene);
            //Set startIndex to new index of end of last gene
            startIndex = dna.indexOf(currGene, startIndex) + currGene.length();
        }
        return sr;
    }
    
    public void testGetAllGenes() {
        //            ATGv  TAAv  ATG   v  v  TGA
        String dna =("ATGATCTAATTTATGCTGCAACGGTGAAGA");
        System.out.println("Testing getAllGenes on :" + dna);
        StorageResource sr = getAllGenes(dna);
        for (String s: sr.data()) {
            System.out.println(s);
        }
        // test on empty string
        dna =("");
        System.out.println("Testing getAllGenes on :" + dna);
        sr = getAllGenes(dna);
        for (String s: sr.data()) {
            System.out.println(s);
        }
    }
    
    public float cgRatio (String dna) {
        float cgRatio = 0;
        int totalStrand = dna.length();
        int numCG = 0;
        for (int i = 0; i < totalStrand; i++) {
            //search through DNA looking for C or G
            if (dna.charAt(i) == 'C' || dna.charAt(i) == 'G') {
                numCG++;
            }
        }
        cgRatio = ((float) numCG)/totalStrand;
        return cgRatio;
    }
    
    public void testCGRatio() {
        String dna = "ATGCCATAG";
        System.out.println(cgRatio(dna));
    }
    
    public int countCTG (String dna) {
        int count = 0;
        int currIndex = dna.indexOf("CTG");
        //If no stop codon, return out of bounds
        while (currIndex != -1 && currIndex != 0) {
            //update currIndex to find next occurrence of the stop codon
            count++;
            currIndex = dna.indexOf("CTG", currIndex+3);
            }
        return count;
    }
    
    public void testCountCTG() {
        String dna = "";//4 CTG
        System.out.println(countCTG(dna));
    }
    
    public void testPrintAllGenes() {
        //            ATGv  TAAv  ATG   v  v  TGA
        String dna =("ATGATCTAATTTATGCTGCAACGGTGAAGA");
        System.out.println("Testing printAllGenes on :" + dna);
        printAllGenes(dna);
        // test on empty string
        dna =("");
        System.out.println("Testing printAllGenes on :" + dna);
        printAllGenes(dna);
    }
    
    public void processGenes (String dna) {
        int numStrings60 = 0;
        int numCG = 0;
        int numGenes = 0;
        int stringLength = 0;
        int maxString = 0;
        int currIndex = 0;
        int indexOfGene = 0;
        String gene = "";
        while (true) {
            if (currIndex == -1){
                break;
            }
            //Find genes
            gene = findSimpleGene(dna, currIndex);
            System.out.println(gene);
            //count num genes
            numGenes++;
            //indexOfGene
            indexOfGene = dna.indexOf(gene, currIndex);
            //update currIndex to beginnign of first index after the gene
            currIndex = indexOfGene + gene.length();
            //update the dna strand to start at the remaining dna
            if (currIndex == -1) {
                break;
            }
            dna = dna.substring(currIndex, dna.length());
            //if gene is longer than 60, than count it
            if (gene.length() > 60) {
                //count and print the #  of strings in sr that are longer than 9 chars
                numStrings60++;
                int currLength = gene.length();
                //find longest gene by comparing this string with the last string
                maxString = Math.max(maxString, currLength);
            }
            //get the cgratio on this gene
            double cgRatio = cgRatio(gene);
            if (cgRatio > 0.35) {
                //if cg ratio is >.35 count it
                numCG++;
            }
        }
        System.out.println("Num genes is: " + numGenes);
        System.out.println("Number of Strings >= 60 chars: " + numStrings60++);
        System.out.println("Number of strings whose CG ratio is higher than 0.35: " +numCG);
        System.out.println("Longest string is this many characters long: " + maxString);
    }
    
    public void testProcessGenes() {
        String dna = "";
        processGenes(dna);
    }
}
    


